{
  "name": "cyberpunk-flying-car-game",
  "version": "0.1.0",
  "description": "A web-based, multiplayer, open-world cyberpunk flying car game",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "nodemon src/server/index.js",
    "build": "parcel build src/client/index.html --dist-dir dist",
    "watch": "parcel watch src/client/index.html --dist-dir dist",
    "dev:all": "concurrently \"npm run dev\" \"npm run watch\""
  },
  "keywords": [
    "cyberpunk",
    "flying-car",
    "game",
    "threejs",
    "websockets",
    "multiplayer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "parcel": "^2.10.0"
  }
}
